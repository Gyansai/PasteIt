<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <!--<style name="AppTheme" parent="AppTheme.Base"/>-->

    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/blue_500</item>
        <item name="colorPrimaryDark">@color/blue_500</item>
        <item name="android:textColorPrimary">@color/grey_1000b</item>
        <item name="android:textColorSecondary">@color/grey_1000b</item>
        <item name="android:textColorTertiary">@color/grey_1000b</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="windowActionModeOverlay">true</item>
    </style>
    <style name="ActionBarThemeOverlay" parent="">
        <item name="colorPrimary">@color/blue_500</item>
        <item name="colorPrimaryDark">@color/blue_500</item>
        <item name="android:textColorPrimary">@color/grey_1000w</item>
        <item name="android:textColorSecondary">@color/grey_1000w</item>
        <item name="android:textColorTertiary">@color/grey_1000w</item>
    </style>
